programa
{
	
	funcao inicio()
	{
		inteiro count[101]

		para(inteiro i = 100; i > -1; i--){
			count[i] = i + 100
		}
		para(inteiro j = 100; j > -1; j--){
			escreva(count[j], "\n")
		}

		
	}
}
